{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "typeRoots": [
      "./typings",
      "./vendor/types"
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/auth/[...nextauth].tsx",
    ".next/auth/redirect.js",
    ".next/auth/[...nextauth].s",
    ".next/auth/[...nextauth].js",
    "pages/api/allocation/[...slug].js"
  ],
  "exclude": [
    "node_modules"
  ]
}